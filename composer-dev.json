{
    "name": "51degrees/fiftyone.geolocation",
    "description": "Geo-Location engines for 51Degrees Pipeline API - Get postal address details from the device connecting to your website or service",   
	"authors" : [
		{
			"name": "Filip Hn√≠zdo",
            "email": "filip@octophin.com",
            "homepage": "https://octophindigital.com/"
        },
		{
			"name": "Steve Ballantine",
            "email": "stephen@51degrees.com"
        },
		{
			"name": "Ben Shilito",
            "email": "ben@51degrees.com"
        },
		{
			"name": "Joseph Dix",
            "email": "joseph@51degrees.com"
        }
    ],
    "license": "EUPL-1.2", 
    "type": "library",
    "autoload": {
        "classmap": [
            "GeoLocationCloud.php",
            "GeoLocationPipelineBuilder.php"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "pipeline-php-core"
        },
        {
            "type": "path",
            "url": "pipeline-php-engines"
        },
        {
            "type": "path",
            "url": "pipeline-php-cloudrequestengine"
        }
    ],
    "require": {
        "51degrees/fiftyone.pipeline.core": "dev-main as 4",
        "51degrees/fiftyone.pipeline.engines": "dev-main as 4",
        "51degrees/fiftyone.pipeline.cloudrequestengine": "dev-main"
    },
    "require-dev": {
        "51degrees/fiftyone.devicedetection": "4.*"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "nodejs-php-fallback/nodejs-php-fallback": false
        }
    }
}
